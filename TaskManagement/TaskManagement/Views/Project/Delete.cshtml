@model Project
@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>
<form id="deleteForm" method="post" class="container">
    <div>
        <label asp-for="Name"></label>
        <input type="text" disabled class="form-control" asp-for="Name" />
    </div>
    <div>
        <label asp-for="Description"></label>
        <input type="text" disabled class="form-control" asp-for="Description" />
    </div>
    <div class="align-content-center pt-2">
        <button type="submit" class="btn btn-danger">Delete</button>
        <a class="btn btn-outline-secondary" asp-action="Index">Back To List</a>
    </div>
</form>



